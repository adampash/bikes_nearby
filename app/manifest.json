{
    "name": "__MSG_appName__",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/chromereload.js",
            "scripts/jquery.js",
            "scripts/_helpers.js",
            "scripts/distance.js",
            "scripts/locate_bikes.js",
            "scripts/background.js"
        ]
    },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "CitiBikes Near",
    "default_popup": "popup.html"
  },
  "permissions": [
    "geolocation",
    "http://www.citibikenyc.com/stations/json",
    "http://www.divvybikes.com/stations/json",
    "https://www.bayareabikeshare.com/stations/json"
  ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://maps.googleapis.com https://maps.gstatic.com; object-src 'self'"
}
